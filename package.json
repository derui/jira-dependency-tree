{
  "name": "jira-dendency-tree",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:css": "tailwindcss --watch -o dist/assets/css/app.css -i ./assets/css/root.css",
    "start:tsc": "tsc --watch",
    "start:vite": "vite",
    "ci": "CI=true vite",
    "start": "concurrently --handle-input \"pnpm:copy-assets\" \"pnpm:start:*\"",
    "build:bundle": "NODE_ENV=production vite build",
    "build:css": "tailwindcss -o dist/assets/css/app.css -i assets/css/root.css",
    "build": "pnpm run build:bundle && pnpm run build:css && pnpm run copy-assets",
    "lint:css": "stylelint --config stylelint.config.cjs 'assets/css/**/*.css'",
    "lint:ts": "eslint --cache --cache-location=node_modules/.cache/eslint src/",
    "lint": "conc \"pnpm:lint:*\"",
    "copy-assets": "cp -r assets/svg/ dist/assets/",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "it:serve": "conc \"msw init dist\" \"pnpm:start:css\" \"pnpm:ci\" \"pnpm:cypress:open\"",
    "it:run": "msw init dist && conc -k true \"pnpm:ci\" \"cypress run --browser chromium\"",
    "cypress:open": "cypress open",
    "deploy:preview": "firebase hosting:channel:deploy preview_name",
    "deploy:production": "firebase deploy",
    "publish:preview": "pnpm run build && pnpm run deploy:preview",
    "publish:production": "pnpm run build && pnpm run deploy:production"
  },
  "author": "derui",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "classnames": "^2.3.2",
    "d3": "^7.8.2",
    "fast-deep-equal": "^3.1.3",
    "immer": "^9.0.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3": "^7.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react": "^3.0.1",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "cypress": "^12.4.1",
    "eslint": "^8.32.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "firebase-tools": "^11.21.0",
    "hygen": "^6.2.11",
    "jsdom": "^21.1.0",
    "jsdom-global": "^3.0.2",
    "msw": "^1.0.0",
    "prettier": "^2.8.3",
    "rome": "^11.0.0",
    "sinon": "^15.0.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "tailwindcss": "^3.2.4",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.3"
  },
  "volta": {
    "node": "18.7.0"
  },
  "msw": {
    "workerDirectory": "dist"
  }
}
