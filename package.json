    "firebase-tools": "^11.19.0",
    "live-server": "^1.2.2",
    "msw": "^0.49.2",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^10.2.0",
    "prettier": "^2.8.1",
