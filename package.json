{
  "name": "jira-dendency-tree",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:css": "tailwindcss --watch -o dist/assets/css/app.css -i ./assets/css/root.css",
    "start:tsc": "tsc --watch",
    "start:vite": "vite",
    "ci": "CI=true vite",
    "start": "concurrently --handle-input \"pnpm:copy-assets\" \"pnpm:start:*\"",
    "build:bundle": "NODE_ENV=production vite build",
    "build:css": "tailwindcss -o dist/assets/css/app.css -i assets/css/root.css",
    "build": "pnpm run build:bundle && pnpm run build:css && pnpm run copy-assets",
    "lint:css": "stylelint --config stylelint.config.cjs 'assets/css/**/*.css'",
    "lint:ts": "eslint --cache --cache-location=node_modules/.cache/eslint src/ test/",
    "lint": "conc \"pnpm:lint:*\"",
    "copy-assets": "cp -r assets/svg/ dist/assets/",
    "test": "ava",
    "test:watch": "ava --watch",
    "it:serve": "conc \"pnpm:start:css\" \"pnpm:ci\" \"pnpm:cypress:open\"",
    "it:run": "conc -k true \"pnpm:ci\" \"cypress run --browser chromium\"",
    "cypress:open": "cypress open",
    "deploy:preview": "firebase hosting:channel:deploy preview_name",
    "deploy:production": "firebase deploy",
    "publish:preview": "pnpm run build && pnpm run deploy:preview",
    "publish:production": "pnpm run build && pnpm run deploy:production"
  },
  "author": "derui",
  "license": "MIT",
  "dependencies": {
    "@cycle/dom": "^23.1.0",
    "@cycle/http": "^15.4.0",
    "@cycle/isolate": "^5.2.0",
