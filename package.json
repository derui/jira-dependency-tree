{
  "name": "jira-dendency-tree",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:tsc": "tsc --watch",
    "start:vite": "vite",
    "ci": "CI=true vite",
    "start": "concurrently --handle-input \"pnpm:copy-assets\" \"pnpm:start:*\"",
    "build:bundle": "NODE_ENV=production vite build",
    "build": "pnpm run build:bundle && pnpm pnpm run copy-assets",
    "lint:css": "stylelint --config stylelint.config.cjs 'assets/css/**/*.css'",
    "lint:ts": "eslint --cache --cache-location=node_modules/.cache/eslint src/",
    "lint": "conc \"pnpm:lint:*\"",
    "copy-assets": "cp -r assets/svg/ dist/assets/",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "it:serve": "conc \"msw init dist\" \"pnpm:start:css\" \"pnpm:ci\" \"pnpm:cypress:open\"",
    "it:run": "msw init dist && conc -k true \"pnpm:ci\" \"cypress run --browser chromium\"",
    "cypress:open": "cypress open",
    "deploy:preview": "firebase hosting:channel:deploy preview_name",
    "deploy:production": "firebase deploy",
    "publish:preview": "pnpm run build && pnpm run deploy:preview",
    "publish:production": "pnpm run build && pnpm run deploy:production"
  },
  "author": "derui",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "classnames": "^2.3.2",
    "d3": "^7.8.4",
    "fast-deep-equal": "^3.1.3",
    "immer": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3": "^7.4.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/sinon": "^10.0.14",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^3.1.0",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.0.1",
    "cypress": "^12.10.0",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "firebase-tools": "^11.27.0",
    "hygen": "^6.2.11",
    "jsdom": "^21.1.1",
    "jsdom-global": "^3.0.2",
    "msw": "^1.2.1",
    "prettier": "^2.8.7",
    "rome": "^12.0.0",
    "sinon": "^15.0.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.30.1"
  },
  "volta": {
    "node": "18.7.0"
  },
  "msw": {
    "workerDirectory": "dist"
  }
