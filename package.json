{
  "name": "jira-dendency-tree",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:css": "postcss --watch -o dist/static/css/app.css assets/css/root.css",
    "start:tsc": "tsc --watch",
    "start:vite": "vite",
    "start": "npm-run-all -p start:tsc start:vite start:css copy-assets",
    "build:bundle": "NODE_ENV=production vite build",
    "build:css": "postcss -o dist/static/css/app.css assets/css/root.css",
    "build": "npm-run-all -s build:css build:bundle",
    "lint:css": "stylelint --config stylelint.config.js 'assets/css/**/*.css'",
    "lint:ts": "eslint --fix --ext .ts,.tsx src/ts/",
    "lint": "npm-run-all -p lint:ts lint:css",
    "copy-assets": "cp -r assets/svg/ dist/static/",
    "test-live-server": "live-server --no-browser --port=8081 test",
    "test-uvu": "node --loader ./loader.js -r esbuild-register node_modules/uvu/bin.js test",
    "test": "npm-run-all -r -p test-live-server test-uvu",
    "test:watch": "chokidar 'src/**/*.ts' 'src/**/*.tsx' 'test/**/*.ts' 'test/**/*.tsx' -c 'pnpm run test-uvu'"
  },
  "author": "derui",
  "license": "MIT",
  "dependencies": {
    "@cycle/dom": "^23.1.0",
    "@cycle/http": "^15.4.0",
    "@cycle/isolate": "^5.2.0",
    "@cycle/run": "^5.7.0",
    "@cycle/state": "^1.7.0",
    "d3": "^7.6.1",
    "d3-hierarchy": "^3.1.2",
    "fast-deep-equal": "^3.1.3",
    "immer": "^9.0.15",
    "snabbdom": "^3.5.1",
    "xstream": "^11.14.0"
  },
  "devDependencies": {
    "@cycle/time": "^0.21.0",
    "@types/d3": "^7.4.0",
    "@types/d3-hierarchy": "^3.1.0",
    "@types/jsdom": "^16.2.15",
    "@types/node": "^18.7.16",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "autoprefixer": "^10.4.8",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "cssnano": "^5.1.13",
    "esbuild": "^0.15.7",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "firebase-tools": "^11.9.0",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "postcss-nesting": "^10.1.10",
    "prettier": "^2.7.1",
    "puppeteer": "^17.1.3",
    "sinon": "^14.0.0",
    "snabbdom-selector": "^5.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3",
    "uvu": "^0.5.6",
    "vite": "^3.1.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "volta": {
    "node": "18.7.0"
  }
}
