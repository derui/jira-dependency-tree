{
  "name": "jira-dendency-tree",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:css": "postcss --watch -o dist/assets/css/app.css assets/css/root.css",
    "start:tsc": "tsc --watch",
    "start:vite": "vite",
    "ci": "CI=true vite",
    "start": "concurrently --handle-input \"pnpm:copy-assets\" \"pnpm:start:*\"",
    "build:bundle": "NODE_ENV=production vite build",
    "build:css": "postcss -o dist/assets/css/app.css assets/css/root.css",
    "build": "pnpm run build:bundle && pnpm run build:css && pnpm run copy-assets",
    "lint:css": "stylelint --config stylelint.config.cjs 'assets/css/**/*.css'",
    "lint:ts": "eslint --fix --ext .ts,.tsx src/",
    "lint": "conc \"pnpm:lint:*\"",
    "copy-assets": "cp -r assets/svg/ dist/assets/",
    "test": "node --loader ./loader.js -r esbuild-register node_modules/uvu/bin.js test",
    "test:watch": "chokidar 'src/**/*.ts' 'src/**/*.tsx' 'test/**/*.ts' 'test/**/*.tsx' -c 'pnpm run test'",
    "e2e:serve": "conc \"pnpm:ci\" \"pnpm:cypress:open\"",
    "cypress:open": "cypress open",
    "deploy:preview": "firebase hosting:channel:deploy preview_name",
    "deploy:production": "firebase deploy",
    "publish:preview": "pnpm run build && pnpm run deploy:preview",
    "publish:production": "pnpm run build && pnpm run deploy:production"
  },
  "author": "derui",
  "license": "MIT",
  "dependencies": {
    "@cycle/dom": "^23.1.0",
    "@cycle/http": "^15.4.0",
    "@cycle/isolate": "^5.2.0",
    "@cycle/run": "^5.7.0",
    "@cycle/state": "^1.7.0",
    "d3": "^7.7.0",
    "fast-deep-equal": "^3.1.3",
    "immer": "^9.0.16",
    "snabbdom": "^3.5.1",
    "xstream": "^11.14.0"
  },
  "devDependencies": {
    "@cycle/time": "^0.21.0",
    "@types/d3": "^7.4.0",
    "@types/node": "^18.11.12",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "autoprefixer": "^10.4.13",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.14",
    "cypress": "^12.0.2",
    "esbuild": "^0.16.3",
    "esbuild-register": "^3.4.1",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "firebase-tools": "^11.16.0",
    "live-server": "^1.2.2",
    "msw": "^0.49.1",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^10.2.0",
    "prettier": "^2.8.1",
    "sinon": "^15.0.0",
    "snabbdom-selector": "^5.0.0",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4",
    "uvu": "^0.5.6",
    "vite": "^4.0.0",
    "vite-tsconfig-paths": "^4.0.2"
  },
  "volta": {
    "node": "18.7.0"
  },
  "msw": {
    "workerDirectory": "dist"
  }
}
