{
  "name": "jira-dendency-tree",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:css": "postcss --watch -o dist/static/css/app.css assets/css/root.css",
    "start:tsc": "tsc --watch",
    "start:vite": "vite",
    "start": "npm-run-all -p start:tsc start:vite start:css",
    "build:bundle": "NODE_ENV=production vite build",
    "build:css": "postcss -o dist/static/css/app.css assets/css/root.css",
    "build": "npm-run-all -s build:css build:bundle",
    "lint:css": "stylelint --config stylelint.config.js 'assets/css/**/*.css'",
    "lint:ts": "eslint --fix --ext .ts,.tsx src/ts/",
    "lint": "npm-run-all -p lint:ts lint:css",
    "test-live-server": "live-server --no-browser --port=8081 test",
    "test-uvu": "uvu -r tsconfig-paths/register -r tsm",
    "test": "npm-run-all -r -p test-live-server test-uvu",
    "test:watch": "chokidar --silent 'src/**/*.ts' 'test/**/*.ts' 'src/**/*.tsx' -c 'pnpm run test-uvu'"
  },
  "author": "derui",
  "license": "MIT",
  "dependencies": {
    "@cycle/dom": "^23.1.0",
    "@cycle/run": "^5.7.0",
    "@cycle/state": "^1.7.0",
    "d3": "^7.6.1",
    "d3-hierarchy": "^3.1.2",
    "jira.js": "^2.15.2",
    "xstream": "^11.14.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/d3-hierarchy": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "autoprefixer": "^10.4.7",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "cssnano": "^5.1.12",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^14.1.0",
    "postcss-nesting": "^10.1.10",
    "prettier": "^2.7.1",
    "puppeteer": "^15.3.2",
    "tsconfig-paths": "^4.0.0",
    "tsm": "^2.2.1",
    "typescript": "^4.7.4",
    "uvu": "^0.5.6",
    "vite": "^2.9.13",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "volta": {
    "node": "16.16.0"
  }
}
